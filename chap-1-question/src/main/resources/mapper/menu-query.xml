<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <entry key="updatePrice">
        UPDATE TBL_DISCOUNT
           SET DISCOUNT_PRICE = ?
         WHERE PRODUCT_NAME = ?
    </entry>

    <entry key="insertOrder">
        INSERT INTO TBL_ORDER(
            ORDER_CODE ,
            MEMBER_CODE ,
            DISCOUNT_CODE
        )VALUES(
            ?,
            ?,
            ?
        )

    </entry>

    <entry key="selectOrder">
        SELECT
            *
        FROM TBL_ORDER
    </entry>

</properties>